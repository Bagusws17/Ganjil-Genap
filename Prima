#include <iostream>  
// Program untuk menentukan bilangan yang di input merupakan bilangan prima atau tidak
using namespace std;  
int main()  
{  
  int n, i, m=0, flag=0;  
  cout << "Masukkan Angka: ";  
  cin >> n;  
  m=n/2;  
  for(i = 2; i <= m; i++)  
  {  
      if(n % i == 0)  
      {  
          cout<<"Angka ini bukan bilangan prima."<<endl;  
          flag=1;  
          break;  
      }  
  }  
  if (flag==0)  
      cout << "Angka ini merupakan bilangan prima."<<endl;  
  return 0;  
} 
